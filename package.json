    "gh-pages": "^2.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.13.0",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.0.0",
