{
  "name": "tenet-styled-layout-components",
  "version": "0.3.1",
  "description": "A shared list of layout styled components to be used for any site",
  "author": "ebello",
  "license": "MIT",
  "repository": "TenetPartners/tenet-styled-layout-components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:updatesnapshots": "npm run test -- --updateSnapshot",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs"
  },
  "dependencies": {
    "styled-components-breakpoint": "^2.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
