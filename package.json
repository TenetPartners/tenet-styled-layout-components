  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/plugin-external-helpers": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/react": "5.3.19",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
